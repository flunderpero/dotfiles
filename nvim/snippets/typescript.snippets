snippet mock "Mock a module"
	vi.mock("${1}", async () => ({
    	...(await vi.importActual<any>("${1}")),
		${2}
	}))

snippet mock_ext "Mock a module (extended)"
	vi.mock("${1}", () => {
    	const mod = await vi.importActual<any>("${1}")
		return {
			...mod,
			${2}
	}})
